#include <iostream>
using namespace std;


void print(){
    cout<<1<<endl;
    
}

int main() {
    print();
    return 0;
}


OUTPUT: 1


#include <iostream>
using namespace std;


void print(){
    cout<<1<<endl;
    print();
}

int main() {
    print();
    return 0;
}



OUTPUT:1
1
1
and so on infinite

